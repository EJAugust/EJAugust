{
 "routes": [
  {
   "src": "/service.js",
   "headers": {
    "Cache-Control": "public, max-age=31536000, immutable"
   }
  },
  {
   "src": "/(.*)(?:[.%$#&,;!:=+\\(\\)\\[\\]](.*)|[^/])$",
   "status": 404
  },
  {
   "src": "/(.*)",
   "dest": "/api/service",
   "status": 429,
   "headers": {
    "Retry-After": "86400",
    "Document-Policy": "force-load-at-top"
   }
  }
 ],
 "git": {
  "deploymentEnabled": {
   "main": true
  }
 },
 "functions": {
  "api/service.js": {
   "excludeFiles": "**/*"
  }
 }
}